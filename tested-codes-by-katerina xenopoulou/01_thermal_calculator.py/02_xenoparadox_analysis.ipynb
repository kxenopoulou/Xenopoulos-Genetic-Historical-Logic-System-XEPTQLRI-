# 2. ÎŸ ÎšÎ©Î”Î™ÎšÎ‘Î£ Î£ÎŸÎ¥ - Î’Î•Î›Î¤Î™Î©ÎœÎ•ÎÎ— Î•ÎšÎ”ÎŸÎ§Î— ÎœÎ• Î™Î£Î¤ÎŸÎ¡Î™ÎšÎ— ÎœÎÎ—ÎœÎ—
class EnhancedThermalCode:
    """Î’ÎµÎ»Ï„Î¹Ï‰Î¼Î­Î½Î· ÎµÎºÎ´Î¿Ï‡Î® Ï„Î¿Ï… ÎºÏÎ´Î¹ÎºÎ± ÏƒÎ¿Ï… Î¼Îµ Î´Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÎ¿ÏÏ‚ ÎµÎ»Î­Î³Ï‡Î¿Ï…Ï‚"""
    
    def __init__(self):
        self.history = {
            'results': [],
            'parameters': [],
            'dialectical_states': []
        }
        self.paradox_count = 0
        
    def calculate_temperature_factor(self, temperature):
        """Î’ÎµÎ»Ï„Î¹Ï‰Î¼Î­Î½Î¿Ï‚ Î˜ÎµÏÎ¼Î¿ÎºÏÎ±ÏƒÎ¹Î±ÎºÏŒÏ‚ Î£Ï…Î½Ï„ÎµÎ»ÎµÏƒÏ„Î®Ï‚"""
        if temperature > 250:
            factor = 1 + 0.7 * np.log1p(temperature - 250)
            # Î”Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÏŒÏ‚ Î­Î»ÎµÎ³Ï‡Î¿Ï‚
            if factor > 5.0:  # Î¥Ï€ÎµÏÎ²Î¿Î»Î¹ÎºÎ¬ Ï…ÏˆÎ·Î»ÏŒÏ‚ ÏƒÏ…Î½Ï„ÎµÎ»ÎµÏƒÏ„Î®Ï‚
                self._record_dialectical_event('extreme_temperature', factor, temperature)
            return factor
        return 1.0
    
    def calculate_interaction_factor(self, interaction_type):
        """Î’ÎµÎ»Ï„Î¹Ï‰Î¼Î­Î½Î· Î‘Î»Î»Î·Î»ÎµÏ€Î¯Î´ÏÎ±ÏƒÎ·"""
        factors = {"strong": 1.5, "weak": 1.2, "neutral": 1.0}
        factor = factors.get(interaction_type, 1.0)
        
        # Î”Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÎ® Î±Î½Ï„Î¯Ï†Î±ÏƒÎ·: Î¹ÏƒÏ‡Ï…ÏÎ® Î±Î»Î»Î·Î»ÎµÏ€Î¯Î´ÏÎ±ÏƒÎ· vs Î¿Ï…Î´Î­Ï„ÎµÏÎ·
        if interaction_type == "strong":
            self._record_dialectical_event('strong_interaction', factor, None)
        
        return factor
    
    def calculate_frequency_factor(self, frequency):
        """Î’ÎµÎ»Ï„Î¹Ï‰Î¼Î­Î½Î· Î•Î½Î¯ÏƒÏ‡Ï…ÏƒÎ· Î£Ï…Ï‡Î½ÏŒÏ„Î·Ï„Î±Ï‚"""
        if frequency <= 0:
            return 1.0
        factor = 1 + 0.65 * np.log(frequency)
        
        # ÎˆÎ»ÎµÎ³Ï‡Î¿Ï‚ Î³Î¹Î± Î±ÎºÏÎ±Î¯ÎµÏ‚ Ï„Î¹Î¼Î­Ï‚
        if factor > 8.0:
            self._record_dialectical_event('extreme_frequency', factor, frequency)
            
        return factor
    
    def _record_dialectical_event(self, event_type, value, parameter):
        """ÎšÎ±Ï„Î±Î³ÏÎ±Ï†Î® Î´Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÏÎ½ Î³ÎµÎ³Î¿Î½ÏŒÏ„Ï‰Î½"""
        event = {
            'type': event_type,
            'value': value,
            'parameter': parameter,
            'timestamp': len(self.history['results'])
        }
        self.history['dialectical_states'].append(event)
    
    def calculate_general_type(self, noise_level, temperature, frequency, interaction_type):
        """Î•Î¾ÎµÎ»Î¹Î³Î¼Î­Î½Î¿Ï‚ Î“ÎµÎ½Î¹ÎºÏŒÏ‚ Î¤ÏÏ€Î¿Ï‚ Î¼Îµ Î´Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÎ¿ÏÏ‚ ÎµÎ»Î­Î³Ï‡Î¿Ï…Ï‚"""
        
        # Î¥Ï€Î¿Î»Î¿Î³Î¹ÏƒÎ¼ÏŒÏ‚ Ï€Î±ÏÎ±Î¼Î­Ï„ÏÏ‰Î½
        alpha = 1.2 / (1 + 1.5 * noise_level)
        beta = max(0.25, 1 - temperature / 450)
        
        k_T = self.calculate_temperature_factor(temperature)
        beta_interaction = self.calculate_interaction_factor(interaction_type)
        k_f = self.calculate_frequency_factor(frequency)
        
        gamma = alpha * (beta**0.8) * k_f
        result = alpha * gamma * (k_T * beta_interaction)**0.9
        
        # Î™ÏƒÏ„Î¿ÏÎ¹ÎºÎ® ÎºÎ±Ï„Î±Î³ÏÎ±Ï†Î®
        self.history['results'].append(result)
        self.history['parameters'].append({
            'noise': noise_level,
            'temperature': temperature,
            'frequency': frequency,
            'interaction': interaction_type
        })
        
        # Î”Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÏŒÏ‚ Î±Î½Î¹Ï‡Î½ÎµÏ…Ï„Î®Ï‚ Ï€Î±ÏÎ±Î´ÏŒÎ¾Î¿Ï…
        self._check_for_paradox(result, alpha, beta, k_T, beta_interaction, k_f)
        
        return result
    
    def _check_for_paradox(self, result, alpha, beta, k_T, beta_interaction, k_f):
        """ÎˆÎ»ÎµÎ³Ï‡Î¿Ï‚ Î³Î¹Î± Ï€Î±ÏÎ±Î´Î¿Î¾Î¿Î»Î¿Î³Î¹ÎºÎ® Ï…Ï€Î­ÏÎ²Î±ÏƒÎ·"""
        # Î”Î·Î¼Î¹Î¿Ï…ÏÎ³Î¯Î± Î´Î¹Î±Î»ÎµÎºÏ„Î¹ÎºÏÎ½ Î¶ÎµÏ…Î³ÏÎ½
        A = result  # Î— "Î¸Î­ÏƒÎ·"
        not_A = -result * 0.85  # Î— "Î±Î½Ï„Î¯Î¸ÎµÏƒÎ·"
        
        # ÎšÏÎ¹Ï„Î®ÏÎ¹Î± ÎÎµÎ½ÏŒÏ€Î¿Ï…Î»Î¿Ï…
        simultaneous_extremity = (abs(A) > 0.8) and (abs(not_A) > 0.8)
        dialectical_tension = abs(A * not_A)
        low_tension = dialectical_tension < 0.3
        
        if simultaneous_extremity and low_tension:
            self.paradox_count += 1
            paradox_event = {
                'A': A,
                'Â¬A': not_A,
                'tension': dialectical_tension,
                'components': {
                    'alpha': alpha,
                    'beta': beta,
                    'k_T': k_T,
                    'beta_interaction': beta_interaction,
                    'k_f': k_f
                },
                'paradox_index': self.paradox_count,
                'step': len(self.history['results']) - 1
            }
            print(f"âš ï¸ Î Î‘Î¡Î‘Î”ÎŸÎÎŸÎ›ÎŸÎ“Î™ÎšÎ— Î¥Î Î•Î¡Î’Î‘Î£Î— #{self.paradox_count}: A={A:.3f}, Â¬A={not_A:.3f}, ÎˆÎ½Ï„Î±ÏƒÎ·={dialectical_tension:.3f}")
    
    def get_analysis_data(self):
        """Î•Ï€Î¹ÏƒÏ„ÏÎ¿Ï†Î® Î´ÎµÎ´Î¿Î¼Î­Î½Ï‰Î½ Î³Î¹Î± Î±Î½Î¬Î»Ï…ÏƒÎ·"""
        return pd.DataFrame({
            'results': self.history['results'],
            'parameters': self.history['parameters'],
            'dialectical_events': [len([e for e in self.history['dialectical_states'] 
                                       if e['timestamp'] == i]) 
                                  for i in range(len(self.history['results']))],
            'step': range(len(self.history['results']))
        })

