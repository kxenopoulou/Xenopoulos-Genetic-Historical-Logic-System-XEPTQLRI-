                    for i in range(X.shape[0]):
                        if np.random.rand() < 0.2:  # 20% Ï„Ï‰Î½ Î´ÎµÎ¹Î³Î¼Î¬Ï„Ï‰Î½
                            # Î•ÎºÏÎ·ÎºÏ„Î¹ÎºÎ® Î´Î¹Î±Ï„Î±ÏÎ±Ï‡Î®
